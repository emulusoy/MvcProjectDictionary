@using EntityLayer.Concrete
@model List<Category>

@{
    Layout = null;
}
  
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />

<table class="table table-bordered">

    <tr>
        <td>ID</td>
        <td>Name</td>
        <td>Description</td>
        <td>Status</td>
        <td>Edit</td>
    </tr>
    @foreach (var x in Model) { 
        <tr>
            <td>@x.CategoryID</td>
            <td>@x.CategoryName</td>
            <td>@x.CategoryDescription</td>
            <td>@x.CategoryStatus</td>
        </tr>
    }
</table>
