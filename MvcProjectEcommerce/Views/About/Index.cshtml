@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>About</h2>
<table class="table table-bordered">

    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Active/Passive</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.AboutID</td>
            <td>Title</td>
            <td>
                <a href="#" class="btn btn-outline-warning">Active</a>
                <a href="#" class="btn btn-outline-warning">Passive</a>
             </td>


        </tr>
    }

</table>

<br />
@Html.Action("AboutPartial","About")